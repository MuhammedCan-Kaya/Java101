import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner input = new Scanner(System.in);
        System.out.print(" Please enter the value whose harmonic sequence you want to find: ");
        int n = input.nextInt();
        double i = 0,total = 0;

        while (n >= 0 && n > i) {
            i++;
            total += 1 / i;
            System.out.print(1 / i + " + " );
        }
        System.out.print(" = " + total);
    }
}
