public class Main {
    public static void main(String[] args) {

        for (int i = 2; i <= 100; i++) {
            int kontrol = 0;
            for (int n = 2; n < i; n++) {
                if (i % n == 0) {
                    kontrol = 1;
                    break;
                }
            }
            if (kontrol == 0) {
                System.out.println(i);
            }
        }
    }
}
